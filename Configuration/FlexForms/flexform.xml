<T3DataStructure>
    <meta>
        <langDisable>1</langDisable>
    </meta>
    <sheets>
        <sheet1>
            <ROOT>
                <TCEforms>
                    <sheetTitle>Title</sheetTitle>
                    <sheetDescription>Descr.</sheetDescription>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.type>
						<TCEforms>
							<label>Ausrichtung</label>
							<onChange>reload</onChange>
							<config>
								<type>select</type>
								<renderType>selectSingle</renderType>
								<items type="array">
									<numIndex index="0" type="array">
										<numIndex index="0">Medium rechts</numIndex>
										<numIndex index="1">mright</numIndex>
									</numIndex>
									<numIndex index="1" type="array">
										<numIndex index="0">Medium links</numIndex>
										<numIndex index="1">mleft</numIndex>
									</numIndex>
								</items>
							</config>
						</TCEforms>
					</settings.type>    
                    <settings.text>
                        <TCEforms>
                            <label>Text</label>
                            <config>
                                <type>text</type>
                                <rows>5</rows>
                                <cols>30</cols>
                                <eval>trim</eval>
                                <max>100</max>
                                <enableRichtext>1</enableRichtext>
                                <richtextConfiguration>default</richtextConfiguration>
                            </config>
                        </TCEforms>
                    </settings.text>
                    <settings.img1>
						<TCEforms>
							<label>Bild</label>
							<config>										
								<type>group</type>
								<internal_type>file</internal_type>
								<allowed>gif, jpg,jpeg,png,csv</allowed>
								<max_size>10000</max_size>
								<uploadfolder>uploads/tx_fsfzollertextmedia</uploadfolder>
								<maxitems>1</maxitems>
								<size>1</size>
								<selectedliststyle>Width:180px</selectedliststyle>
							</config>
						</TCEforms>
					</settings.img1>
                    <settings.overlayimage>
                        <TCEforms>
                            <label>Video Thumbnail</label>
                            <config>
                                <type>group</type>
                                <internal_type>file</internal_type>
                                <allowed>gif, jpg,jpeg,png,csv</allowed>
                                <max_size>10000</max_size>
                                <uploadfolder>uploads/tx_fsfzollertextmedia</uploadfolder>
                                <maxitems>1</maxitems>
                                <size>1</size>
                                <selectedliststyle>Width:180px</selectedliststyle>
                            </config>
                        </TCEforms>
                    </settings.overlayimage>						
                    <settings.youtube>
                        <TCEforms>
                            <label>Youtube ID</label>
                            <config>
                                <type>input</type>
                                <eval>trim</eval>
                                <size>30</size>
                            </config>
                        </TCEforms>
                    </settings.youtube>
                    <settings.youtubestart>
                        <TCEforms>
                            <label>Youtube Start (sec)</label>
                            <config>
                                <type>input</type>
                                <eval>trim</eval>
                                <size>30</size>
                            </config>
                        </TCEforms>
                    </settings.youtubestart>
                    <settings.youtubeende>
                        <TCEforms>
                            <label>Youtube Ende (sec)</label>
                            <config>
                                <type>input</type>
                                <eval>trim</eval>
                                <size>30</size>
                            </config>
                        </TCEforms>
                    </settings.youtubeende>
                </el>
            </ROOT>
        </sheet1>
    </sheets>
</T3DataStructure>
